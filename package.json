{
  "name": "bunnymq",
  "version": "1.2.0",
  "description": "BunnyMq is a RabbitMq wrapper",
  "keywords": [
    "rabbitmq",
    "broker",
    "amqp",
    "taskQueue",
    "message",
    "publish/subscribe"
  ],
  "main": "index.js",
  "scripts": {
    "test": "jshint . && mocha --recursive test --timeout=5000",
    "start": "node index.js",
    "cover": "jshint . && istanbul cover _mocha -- --recursive test --timeout=5000",
    "doc": "jsdoc -d ./docs --readme README.md --package package.json --template node_modules/minami lib/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dial-once/node-bunnymq.git"
  },
  "author": "khatal.yacine@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dial-once/node-bunnymq/issues"
  },
  "homepage": "https://github.com/dial-once/node-bunnymq#readme",
  "dependencies": {
    "amqplib": "^0.4.1",
    "node-uuid": "^1.4.7"
  },
  "devDependencies": {
    "assert": "1.3.0",
    "minami": "1.1.1"
  }
}
